{
  "name": "shed",
  "version": "0.1.0",
  "description": "Cross-platform Dev Dependency Manager TUI (macOS & Windows)",
  "type": "module",
  "bin": {
    "shed": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "tsx src/index.ts",
